<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>HRA_ChartSeriesRuleForPieChart</name>
        <uuid>_a-0000dde1-361a-8000-9ccd-01ef9001ef90_39003</uuid>
        <description>Contains the value for the chart series rule for Pie chart</description>
        <parentUuid>_a-0000dd0b-90ea-8000-9ccd-01ef9001ef90_18584</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#&quot;SYSTEM_SYSRULES_chartSeries&quot;(
  label: concat(
    ri!SkillName,
    &quot;(%)&quot;
  ),
  data: {
    tointeger(
      concat(
        #&quot;_a-0000dd0b-90ea-8000-9ccd-01ef9001ef90_19682&quot;(
          id: ri!ProficiencyId
        ).percentage,
        &quot;%&quot;
      )
    )
  }
)</definition>
        <namedTypedValue>
            <name>SkillName</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>ProficiencyId</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
