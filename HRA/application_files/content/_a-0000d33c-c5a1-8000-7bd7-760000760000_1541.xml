        <uuid>_a-0000d33c-c5a1-8000-7bd7-760000760000_1541</uuid>
        <description>Returns true() -- if list is empty or null
false() --- list is not empty</description>
        <parentUuid>0000d157-5f3c-8000-7e4e-0a000064044c</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>=if(isnull(ri!List),true(),if(length(ri!List)&gt;0,false(),true()))</definition>
        <namedTypedValue>
            <name>List</name>
            <type>
